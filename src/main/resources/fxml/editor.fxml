<?xml version="1.0" encoding="UTF-8"?>

<?import animtool.gui.media.DynamicImageView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<BorderPane fx:id="rootPane" prefHeight="623.0" prefWidth="873.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="animtool.gui.editor.EditorController" onMouseEntered="#rootPaneOnMouseEntered" onMouseExited="#rootPaneOnMouseExited">
    <center>
        <DynamicImageView fx:id="previewImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                          preserveRatio="true" BorderPane.alignment="CENTER"/>
    </center>
    <bottom>
        <VBox spacing="5.0" fx:id="controlsVBox">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
            <HBox alignment="CENTER" spacing="5.0">
                <Button mnemonicParsing="false" text="&lt;"/>
                <Button mnemonicParsing="false" onAction="#playButtonOnAction" text="Play"/>
                <Button mnemonicParsing="false" text="&gt;"/>
            </HBox>
            <ListView fx:id="timeLineListView" orientation="HORIZONTAL" prefHeight="200.0"/>
        </VBox>
    </bottom>
</BorderPane>
